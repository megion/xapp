<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	                    http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="beanFactory" class="org.springframework.context.support.ClassPathXmlApplicationContext">
        <constructor-arg>
            <array>
                <value>META-INF/spring/datasource.xml</value>
                <value>META-INF/spring/jaxws-client.xml</value>
                <value>META-INF/spring/common.xml</value>
            </array>
        </constructor-arg>
    </bean>
</beans>